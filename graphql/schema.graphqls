# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Todo {
  id: Int!
  text: String!
  done: Boolean!
  user: User!
}

type User {
  id: Int!
  name: String!
}

type Query {
  todos: [Todo!]!
}

input CreateTodoInput {
  text: String!
  userId: Int!
}

type Mutation {
  createTodo(input: CreateTodoInput!): Todo!
}
